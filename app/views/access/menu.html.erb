<h1>Admin Area</h1>

<table>
  <% @subjects.each do |subject| %>
  <th>#</th>
  <th>Subject name</th>
  <tr>
    <td><%= subject.id %></td>
    <td><%= subject.name %></td>
    <td><%= link_to("Show", subject_path(subject.id)) %></td>
    <td><%= link_to("Edit", edit_subject_path(subject.id)) %></td>
    <td><%= link_to("Delete", delete_subject_path(subject.id)) %></td>
  </tr><br>
  <% end %>
</table>
<%= link_to("Log Out", access_logout_path) %>
